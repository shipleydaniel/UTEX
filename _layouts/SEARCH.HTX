<Script Language="javascript">
function  getHreffromVpath(url)
{
    if ("<%CiCatalog%>" != "OWS_Search_Proxy")
    {
	return url;
    }
    else
    {
    	FileLoc = new String(url);
    	FileLoc = FileLoc.substring(FileLoc.indexOf('/')+1);
	newUrl = FileLoc.substring(FileLoc.indexOf('/'));
	return newUrl;
    }
}
function showHref(url, fName)
{
    href = getHreffromVpath(url);
    document.write("<a href=\"" + href + "\">" + fName + "</a>");
}
function showSearchText()
{
    if ("<%HighChar%>" == "1") 
    {
	var L_HighSearch_Text = "<b>Please renter the search string:</b>";
        document.write(L_HighSearch_Text);
    }
    else
    {
	var L_Search_Text = "<b>Search for documents containing text:</b>";
        document.write(L_Search_Text);
    }
}
function showSize(dSize)
{
var L_KB_Text = " KB";
sSize = dSize.replace(/(\D+)/g,'');
kbSize = Math.ceil(parseInt(sSize)/1024)
document.write(kbSize + L_KB_Text);
}
function showGif(url, fName)
{
var L_DocImageAlt_Text = "Icon";
sExtension = fName.substring(fName.length-4,fName.length);
switch (sExtension.toLowerCase()) {
case ".doc":
gifPath="icdoc.gif";
break;
case ".xls":
gifPath="icxls.gif";
break;
case ".ppt":
gifPath="icppt.gif";
break;
case ".htm":
gifPath="ichtm.gif";
break;
case ".pub":
gifPath="icpub.gif";
break;
case ".asp":
gifPath="icasp.gif";
break;
case ".xml":
gifPath="icxml.gif";
break;
case ".jpg":
gifPath="icjpg.gif";
break;
case ".gif":
gifPath="icgif.gif";
break;
case ".txt":
gifPath="ictxt.gif";
break;
case ".png":
gifPath="icpng.gif";
break;
default:
gifPath="icgen.gif";
break;
}
    href = getHreffromVpath(url);
    document.write("<a href=\"" + href + "\"><img alt=\"" + L_DocImageAlt_Text + "\" border=0 src='images/"+gifPath +"'></a>");
}
function SubmitSearch()
{
    f = document.frmSearch;
    var searchText = f.elements["SearchString"].value;
    if ("" == searchText)
    {
	var L_Enter_Text = "Please enter one or more search words.";
	alert(L_Enter_Text);
    }
    else
    {
	f.elements["SearchString"].value = searchText.replace(/\"/g, "");
	f.submit();
    }
}
function CheckRedirect()
{
    if ( ("<%SearchPage%>" == "1") || ("<%HighChar%>" == "1") )
	    document.frmSearch.elements["SearchString"].value = "";
}
</Script>
